/*reset*/
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td{
	margin:0;
	padding:0;
}
body{
	font-size:12px;
	font-family:"微软雅黑", "MS Serif", "New York", "宋体";
	background:#f2f2f2;
}
table{
	border-collapse:collapse;
	border-spacing:0;
}
fieldset,img{
	border:none;
} 
ol,ul{
	list-style:none;
} 
h1,h2,h3,h4,h5,h6{
	font-size:100%;
}
input, textarea, select{
	*font-size:100%;
}
a{
	color:#595959;
	text-decoration:none;
}
a:hover{
	color:#595959;
	text-decoration:underline;
}
/*Global*/
.left{
	float:left;
}
.right{
	float:right;
}
.clean:before, .clean:after{
	content:"";
	display:table;
}
.clean:after{
	clear:both;
}
.clean{
	zoom:1;
    overflow:hidden;
}
body {
	font-size:13px;
	font-family:Arial, "宋体";
	width:100%;
	height:100%;
	background-color:#fff;
}
.box {
	width:795px;
	margin:0 auto;
	background:url("../images/cw/logobg.jpg") no-repeat 291px center;
}
.body {
	border:solid 1px #c2e6f7;
	background:#FFF;
	padding-bottom:50px;
	*padding-bottom:0px;

}
.body-title {
	width:100%;
	height:51px;
	line-height:50px; 
	background:url("../images/cw/titlebg.jpg") repeat-x;
	margin-bottom:20px;
	font-size:14px;
}
.body-title a {
	display:inline-block;
	width:250px;
	height:51px;
	text-align:center;
	text-decoration:none;
	font-size:16px;
}
.body-title span{
	display:block;
	padding-left:50px;
}
.email-tab {
	background:url("../images/icons.gif") no-repeat 40px -82px;
	border-right:solid 1px #CCC;
	color:#888;
}
.phone-tab {
	background:url("../images/icons.gif") no-repeat 50px -35px;
	border-right:solid 1px #CCC;
	color:#888;
}
.email-tab-active {
	border-top-left-radius:10px;
	background:url("../images/icons.gif") #FFF no-repeat 40px -82px;
	border-right:solid 1px #CCC;
	color:#333333;
	font-weight:bold;
}
.phone-tab-active {
	background:url("../images/icons.gif") #FFF no-repeat 50px -35px;
	border-right:solid 1px #CCC;
	color:#333333;
	font-weight:bold;
}
.top-menu {
	text-align:right;
	padding-top:20px;
	line-height:22px;
}
.footer {
	text-align:center;
	color:#666;
	margin-top:20px;
	font-size:12px;
}
.footer a {
	color:#666;
}
.logo{
	display:block;
    width: 256px;
    height: 48px;
    margin-left: -16px;
	text-indent:-9999999px;
	background:url(../images/cw/vvku-logo3.png) no-repeat left center;
    _background:none;
    _filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src = 'http://164service.kkyoo.com/themes/g3/passport/images/cw/vvku-logo3.png');
    _position: relative;
    _cursor: pointer;
}
