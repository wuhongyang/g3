<script type="text/javascript">
$(function () {
	var menus = $('#site-nav').find('.menu');
	//判断是否为IE6
	if ($.browser.msie && ($.browser.version == "6.0") && !$.support.style) {
				$(this).find('.menu-hd').addClass('ie6hd topnav-cur');
		menus.each(function () {
			$(this).mouseover(function () {
				$(this).find('.menu-hd').find('b').addClass('ie6b');
				$(this).find('.menu-bd').show();
			});
			$(this).mouseout(function () {
				$(this).find('.menu-hd').removeClass('ie6hd topnav-cur');
				$(this).find('.menu-hd').find('b').removeClass('ie6b');
				$(this).find('.menu-bd').hide();
			});
		});
	}
	var boxs = $('.box-rank');
	boxs.each(function () {
		var tabs = $(this).find('.tab');
		tabs.each(function (index) {
			$(this).click(function () {
				var conts = $(this).parent().find('.box-rank-list');
				conts.each(function () {
					$(this).addClass('none');
				});
				tabs.each(function () {
					$(this).removeClass('selected');
				});
				conts.eq(index).removeClass('none');
				$(this).addClass('selected');
			});
		});
	});
	
	var boxs = $('.box-ranktwo');
	boxs.each(function () {
		var tabs = $(this).find('.tab');
		tabs.each(function (index) {
			$(this).click(function () {
				var conts = $(this).parent().parent().find('.box-rank-list');
				conts.each(function () {
					$(this).addClass('none');
				});
				tabs.each(function () {
					$(this).removeClass('selected');
				});
				conts.eq(index).removeClass('none');
				conts.eq(index+3).removeClass('none');
				$(this).addClass('selected');
			});
		});
	});
	
});

