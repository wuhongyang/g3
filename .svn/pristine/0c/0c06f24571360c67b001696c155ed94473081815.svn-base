.clearfix {
    zoom: 1;
}
.clearfix:after {
    content: ".";
    display: block;
    clear: both;
    height: 0;
    visibility: hidden;
}
.gray{
	color:#8d8c8c;
}
.left{
	float:left;
}
.right{
	float:right;
}
/*微博外套*/
.weibo-wrap{
	padding:0 20px;
	background:#fff;
}
.weibo-wrap .weibo-main{
	width:630px;
	padding-right:20px;
	border-right:2px solid #f4eae8;
	float:left;
	min-height:500px;
	padding-bottom:50px;
}
.weibo-wrap .weibo-sidebar{
	width:260px;
	padding:0 10px;
}
/*微博表单*/
.weibo-main .textarea textarea{
	border-radius:5px;
	border:solid 1px #CCC;
	display:block;
	width:100%;
	height:80px;
}
.weibo-main .publish{
	text-align:right;
	margin-top:5px;
}
.weibo-main .publish-title{
	background:url("../images/publish-title.gif") no-repeat left center;
	height:30px;
    line-height:30px;
	margin:10px 0;
}
.weibo-main .publish-title .num-word{
    font-size: 14px;
    color: #444;
    font-weight: normal;
}
.weibo-main .publish-title .num-word span{
    font-weight: 700;
    font-size: 22px;
    font-family: Constantia,Georgia;
}
.weibo-main .box-commit .fenlei{
	float:left;
}
/*微博内容区*/
.weibo-main .weibo-content{
    background:#FFF;
	border:solid 1px #d7d7d7;
    margin-top: -1px;
}
.weibo-main .weibo-tab{
    lear: both;
    *position: relative;
	margin-top:20px;
}
.weibo-main .weibo-tab li{
	float:left;
}
.weibo-main .weibo-tab li a{
	border:solid 1px #d7d7d7;
	color:#505050;
	display:block;
	padding:10px 28px;
    font-size: 15px;
}
.weibo-main .weibo-tab .active a{
	border-bottom:1px solid #fff;
	background:#FFF;
}
.weibo-main .weibo-tab .tab-left a{
}
.weibo-main .weibo-tab .tab-center a{
	border-left:none;
	border-right:1px solid #d7d7d7;
}
.weibo-main .weibo-tab .tab-right a{
	background:#efefef;
	border-left:none;
}
.weibo-main .weibo-rows{
	border-bottom:dotted 1px #CCC;
	margin:10px;
	padding:10px;
}
.weibo-main .weibo-rows .weibo-face{
	float:left;
}
.weibo-main .weibo-rows .weibo-face a{
	background:#CCC;
	display:block;
	width:50px;
	height:50px;
}
.weibo-main .weibo-rows .weibo-row{
	width:90%;
}
.weibo-main .weibo-rows .weibo-row .weibo-row{
	width:auto;
	padding:12px;
	margin-top:5px;
	background:#f8f4ed;
	border:solid 1px #e8e4de;
}
.weibo-main .weibo-rows .weibo-row .weibo-msg{
	line-height:20px;
	white-space:normal;
	overflow:hidden;
	color:#666;
	font-size: 13px;
	word-wrap: break-word;
}
.weibo-main .weibo-rows .weibo-row .weibo-msg a{
	color: #0370da;
	font-size: 16px;
}
.weibo-main .weibo-rows .weibo-row .weibo-msg .share-media{
	padding:10px;
}
.weibo-main .weibo-rows .weibo-info{
	margin-top:15px;
}
.weibo-main .weibo-rows .weibo-info .weibo-date a{
	color:#878787;
	font-size: 12px;
}
.weibo-main .weibo-rows .weibo-info .weibo-reply a{
	color:#8d8d8c;
	font-size:12px;
}
.weibo-main .weibo-rows .weibo-info .weibo-reply a:hover{
	text-decoration:underline;
}
/*我的微博侧边栏*/
.weibo-sidebar h1{
	color:#f17542;
	font-size:16px;
	font-weight:bold;
	padding:10px;
	border-bottom:1px dotted #ffba9a;
}
.weibo-sidebar .my-userinfo{
	background:#FFF;
