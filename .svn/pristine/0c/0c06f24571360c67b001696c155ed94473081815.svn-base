	padding:10px 10px 0 10px;
}
.weibo-sidebar .recommend-user .user-face a{
	display:block;
	width:50px;
	height:50px;
	background:#CCC;
}
.weibo-sidebar .recommend-user .user-info{
	margin-left:10px;
	line-height:25px;
}
.follow-link{
	display:block;
	width:62px;
	height:23px;
	color:#2183aa;
	text-align:center;
	border:1px solid #b5d3ed;
	background:#daebff;
}
.follow-link:hover{
	text-decoration:underline;
}
.relay-wbform{
	margin:10px;
	padding:10px;
	background:#DDD;
	border:solid 1px #CCC;
}
.relay-wbform textarea{
	display:block;
	width:100%;
}
.relay-submit{
	padding:0; 
	margin:0;
	text-indent:-9999999px;
	width:60px;
	height:20px;
	border:none;
	cursor:pointer;
	margin:10px 0 10px 10px;
	background:url("../images/relay-submit.gif") no-repeat center;
}
