	margin-bottom:5px;
}
.weibo-sidebar .my-weibo{
	margin-top:10px;
	background:#f6e2d9;
}
.weibo-sidebar .my-weibo li{
	float:left;
	text-align:center;
}
.weibo-sidebar .my-weibo li a{
	display:block;
	width:80px;
	padding:10px 0;
	font-size:14px;
